<script type="text/javascript">
    function enterSubmit() {
        if (event.which == 13) {
            event.preventDefault();
            $('#btn-submit').click();
        }
    }

    $.validator.addMethod("checkUserName", function (value, element) {
        var isSuccess = false;
        $.ajax({
            url: "/Join/UserIdCheck",
            type: "POST",
            async: false,
            data: { user_id: value },
            success: function (data) {
                isSuccess = data.code == 1 ? true : false;
            }
        });

        return isSuccess;
    });

    

	$(function () {
	    $(".menu").dropit();

	    $('#btn-cancel').click(function () {
	        document.location.href = GetUrlBySiteCode();
	    });

		$('#btn-submit').click(function () {
		    if ($('form').valid()) {
		        $.ajax({
		            url: "/FindPassword/FindPasswordValid",
		            type: "POST",
		            async: false,
		            data: { uname: $('#uname').val() },
		            success: function (e) {
		                if (e.code == 0) {
		                    document.location.href = "/FindPassword/FindPassword2" + "?sitecode=CF"
		                }
		            }
		        });
		    }
		});

		$.validator.addMethod("regx", function (value, element, regexpr) {
		    return regexpr.test(value);
		});

		$('form').validate({
			rules: {
			    uname: {
			        required: true,
			        minlength: 6,
			        maxlength: 15,
			        regx: /^[a-z0-9]*$/,
			        checkUserName: true
			    },
            	
			},
			messages: {
			    uname: {
			        required: 'Username is required.',
			        minlength: 'Username must be at least 6 to 15 characters in length.',
			        maxlength: 'Username must be at least 6 to 15 characters in length.',
			        regx: 'The following are allowed in Username : a-z, 0-9, No space &amp; special characters.',
			        checkUserName: 'The Username is incorrect. Please try again.'
			    }

			},			
			highlight: function (element) {
				var id_attr = "#" + $(element).attr("id") + "1";
				$(element).closest('.form-group').addClass('has-error');
				$(id_attr).addClass('fa-exclamation-circle');				
			},
			unhighlight: function (element) {
				var id_attr = "#" + $(element).attr("id") + "1";
				$(element).closest('.form-group').removeClass('has-error');
				$(id_attr).removeClass('fa-exclamation-circle');				
			},
			errorElement: 'span',
			errorClass: 'help-block',
			errorPlacement: function (error, element) {
				if (element.attr("name") == "dob_month" || element.attr("name") == "dob_day") {
					error.appendTo(element.parent("div").next("div"));
				} else {
					error.insertAfter(element);
				}
			}
		});
	});
</script>

